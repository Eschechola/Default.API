{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SqlServer": "<user-secrets>"
  },
  "Hash": {
    "Salt": "<user-secrets>",
    "TimeCost": 3,
    "Threads": 4,
    "Lanes": 4
  },
  "Jwt": {
    "Issuer": "default.com.br",
    "Audience": "Default",
    "Secret": "<user-secrets>",
    "AccessTokenExpirationMinutes": 60
  }
}
